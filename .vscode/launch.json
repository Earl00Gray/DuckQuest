{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}\\src\\server${pathSeparator}${env:PYTHONPATH}"
            },
        },
        {
            "name": "Python: Main",
            "type": "python",
            "request": "launch",
            "program": "src/server/main.py",
            "justMyCode": true,
        }
    ]
}